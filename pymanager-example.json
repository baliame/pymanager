{
	"modules": {
		"pymutils.http_verifier": {
			"verifiers": ["HttpOkVerifier"]
		}
	},
	"processes": {
		"test": {
			"executable": "python3",
			"arguments": ["test.py"],
			"options": {
				"suppress_output": false,
				"environment_file": "test.env"
			},
			"verifier": {
				"type": "pymutils.http_verifier.HttpOkVerifier",
				"arguments": {
					"url": "http://google.com"
				}
			}
		}
	},
	"http": {
		"enabled": true,
		"port": 5001
	},
	"keep_alive": false,
	"messages": [
		"process.exit",
		"verifier.verbose"
	]
}
